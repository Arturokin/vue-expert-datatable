{
  "name": "vue-expert-datatable",
  "version": "0.1.2",
  "description": "Vue component which automates the working with tables, allowing to use in-line editing and API REST based adding, editing and deleting records",
  "main": "dist/vue-expert-datatable.ssr.js",
  "browser": "dist/vue-expert-datatable.esm.js",
  "module": "dist/vue-expert-datatable.esm.js",
  "unpkg": "dist/vue-expert-datatable.min.js",
  "types": "vue-expert-datatable.d.ts",
  "files": [
    "dist/*",
    "vue-expert-datatable.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "ant-design-vue": "^1.7.8",
    "autonumeric": "^4.6.0",
    "axios": "^0.21.1",
    "core-js": "^3.14.0",
    "vee-validate": "^3.4.14",
    "vue-autonumeric": "^1.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/cli-service-global": "^4.5.13",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.34.1",
    "sass-loader": "^10.2.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
